			<xs:selector xpath="sequences/sequence/clips/seqclipref"/>
			<xs:field xpath="@clip-id"/>
		</xs:keyref>
	</xs:element>
	<xs:element name="output">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DocumentItemType">
					<xs:sequence>
						<xs:element ref="params" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="type" type="xs:string"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="track">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DocumentItemType">
					<xs:sequence>
						<xs:element name="clips" minOccurs="0">
							<xs:complexType>
								<xs:choice minOccurs="0" maxOccurs="unbounded">
									<xs:group ref="AnyClipGroup"/>
									<xs:element name="null"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="output" type="Guid"/>
					<xs:attribute name="target" type="xs:string"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ClipType">
		<xs:complexContent>
			<xs:extension base="DocumentItemType">
				<xs:attribute name="dur" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:float">
							<xs:minExclusive value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="align" type="xs:nonNegativeInteger"/>
				<xs:attribute name="target" type="xs:string"/>
				<xs:attribute name="output" type="Guid"/>
				<xs:attribute name="ui-row" type="xs:nonNegativeInteger"/>
				<xs:attribute name="ui-col" type="xs:nonNegativeInteger"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="clip">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ClipType">
					<xs:sequence minOccurs="0">
						<xs:any namespace="##other"/>
					</xs:sequence>
					<xs:anyAttribute namespace="##other"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="stepclip">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ClipType">
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="step" type="xs:float"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="transport">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="params" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="bpm">
				<xs:simpleType>
					<xs:restriction base="xs:float">
						<xs:minExclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="dur">
				<xs:simpleType>
					<xs:restriction base="xs:float">
						<xs:minExclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="align" type="xs:nonNegativeInteger"/>
			<xs:attribute name="type" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ClipRefType">
		<xs:complexContent>
			<xs:extension base="DocumentItemType">
				<xs:attribute name="clip-id" type="Guid" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="clipref" type="ClipRefType"/>
	<xs:element name="sequence">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DocumentItemType">
					<xs:sequence>
						<xs:element name="clips" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="seqclipref" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="dur" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minExclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="seqclipref">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ClipRefType">
					<xs:attribute name="start" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minInclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="dur">
						<xs:simpleType>
							<xs:restriction base="xs:float">
								<xs:minExclusive value="0"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
