<Window x:Class="Animator.Test.TestWindow1"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="TestWindow1" Height="601" Width="907" Style="{StaticResource MainWindowStyle}" xmlns:my="clr-namespace:Animator.UI.Panes" xmlns:my1="clr-namespace:Animator.UI.Editors">
	<Grid>
		<my:TransportPane HorizontalAlignment="Left" Margin="27,22,0,0" x:Name="transportPane1" VerticalAlignment="Top" />
		<my:DocumentPropertiesPane HorizontalAlignment="Left" Margin="81,121,0,0" x:Name="documentPropertiesPane1" VerticalAlignment="Top" Width="292" ShowCounts="{Binding ElementName=showDocCountsCheckBox, Path=IsChecked}" PaneHeaderVisible="{Binding ElementName=showPaneHeaderCheckBox, Path=IsChecked}" />
		<CheckBox Content="Show Doc Item Counts" Height="16" HorizontalAlignment="Left" Margin="156,99,0,0" Name="showDocCountsCheckBox" VerticalAlignment="Top" IsChecked="True" />
		<Button Content="Attach New Document" HorizontalAlignment="Left" Margin="199,22,0,0" Name="button1" VerticalAlignment="Top" Click="button1_Click" />
		<my:OutputPropertiesPane HorizontalAlignment="Left" Margin="456,89,0,0" x:Name="outputPropertiesPane1" VerticalAlignment="Top" Width="286" PaneHeaderVisible="{Binding ElementName=showPaneHeaderCheckBox, Path=IsChecked}" />
		<CheckBox Content="Show Pane Header" Height="16" HorizontalAlignment="Left" Margin="400,44,0,0" Name="showPaneHeaderCheckBox" VerticalAlignment="Top" IsChecked="True" />
		<my1:StepListEditor HorizontalAlignment="Left" Margin="400,292,0,0" x:Name="stepListEditor1" VerticalAlignment="Top" Height="204" Width="436" />
	</Grid>
</Window>
