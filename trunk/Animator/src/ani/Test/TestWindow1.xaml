		<my:TransportPane HorizontalAlignment="Left" Margin="27,22,0,0" x:Name="transportPane1" VerticalAlignment="Top" />
		<my:DocumentPropertiesPane HorizontalAlignment="Left" Margin="81,121,0,0" x:Name="documentPropertiesPane1" VerticalAlignment="Top" Width="292" ShowCounts="{Binding ElementName=showDocCountsCheckBox, Path=IsChecked}" />
		<CheckBox Content="Show Doc Item Counts" Height="16" HorizontalAlignment="Left" Margin="156,99,0,0" Name="showDocCountsCheckBox" VerticalAlignment="Top" IsChecked="True" />
		<Button Content="Attach New Document" HorizontalAlignment="Left" Margin="199,22,0,0" Name="button1" VerticalAlignment="Top" Click="button1_Click" />
		<my:OutputPropertiesPane HorizontalAlignment="Left" Margin="456,160,0,0" x:Name="outputPropertiesPane1" VerticalAlignment="Top" Width="286" />
	</Grid>
</Window>
