<Window x:Class="Animator.UI.Dialogs.TrackPropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Track Properties" Style="{StaticResource SubWindowStyle}" WindowStyle="None" xmlns:panes="clr-namespace:Animator.UI.Panes" ShowInTaskbar="False" SizeToContent="WidthAndHeight">
	<DockPanel MinWidth="200" Margin="2">
		<StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" FlowDirection="RightToLeft">
			<Button Content="Close" IsDefault="True" Click="CloseButton_Click" />
		</StackPanel>
		<panes:TrackPropertiesPane x:Name="trackPropertiesPane">
		</panes:TrackPropertiesPane>
	</DockPanel>
</Window>
