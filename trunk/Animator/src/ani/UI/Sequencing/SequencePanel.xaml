<UserControl x:Class="Animator.UI.Sequencing.SequencePanel"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:seqdata="clr-namespace:Animator.Core.Model.Sequences;assembly=anicore"
			 xmlns:appCore="clr-namespace:Animator.AppCore"
			 xmlns:seq="clr-namespace:Animator.UI.Sequencing" mc:Ignorable="d" 
			 d:DesignHeight="500" d:DesignWidth="1000" Style="{DynamicResource HeaderedPaneStyle}"
			 DataContextChanged="OnDataContextChanged"
			 appCore:AniUI.PaneHeader="Sequence">
	<!--<UserControl.Resources>
		<ContentControl x:Key="HeaderContent">
			<TextBlock>
				<TextBlock.Text>
					<Binding Path="Name" StringFormat="Sequence [{0}]" />
				</TextBlock.Text>
			</TextBlock>
		</ContentControl>
	</UserControl.Resources>-->
	<Grid>
		<ListBox Background="{x:Null}" ItemsSource="{Binding Path=Tracks}" HorizontalContentAlignment="Stretch">
			<ListBox.ItemTemplate>
				<DataTemplate DataType="{x:Type seqdata:SequenceTrack}">
					<seq:SequenceTrackPane />
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
	</Grid>
</UserControl>
