<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:AnimatorPrototype"
	mc:Ignorable="d"
	x:Class="AnimatorPrototype.TimelineGroup"
	x:Name="UserControl">

	<StackPanel x:Name="LayoutRoot">
		<local:ValueTimeline Width="Auto" d:LayoutOverrides="Height" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" BorderThickness="1"/>
		<local:ValueTimeline Width="Auto" d:LayoutOverrides="Height" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" BorderThickness="1"/>
		<local:ValueTimeline Width="Auto" d:LayoutOverrides="Height" BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" BorderThickness="1"/>
	</StackPanel>
</UserControl>