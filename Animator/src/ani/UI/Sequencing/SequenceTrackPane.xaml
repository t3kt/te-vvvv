<UserControl x:Class="Animator.UI.Sequencing.SequenceTrackPane"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:appCore="clr-namespace:Animator.AppCore"
			 mc:Ignorable="d" 
			 d:DesignHeight="150" d:DesignWidth="800" Padding="0">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="150" />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>
		<ContentControl Style="{StaticResource HeaderedPaneStyle}" appCore:AniUI.PaneHeader="Track">
			<Grid>
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto" />
					<RowDefinition />
				</Grid.RowDefinitions>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="Auto"/>
					<ColumnDefinition />
				</Grid.ColumnDefinitions>
				<TextBlock Text="Name:" />
				<TextBox Grid.Column="1" Text="{Binding Path=Name}"/>
			</Grid>
		</ContentControl>
		<ContentControl Grid.Column="1" Style="{StaticResource PaneUserControlStyle}">
			stuff
		</ContentControl>
	</Grid>
</UserControl>
