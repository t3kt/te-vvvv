		<DrawingBrush.Drawing>
			<DrawingGroup>
				<DrawingGroup.Children>
					<GeometryDrawing Geometry="M 2,2L 14,14M 2,14L 14,2">
						<GeometryDrawing.Pen>
							<Pen StartLineCap="Round" EndLineCap="Round" LineJoin="Round" Brush="#FF4B5662"/>
